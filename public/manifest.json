{
          "manifest_version": 3,
          "name": "Tab Manager",
          "version": "1.0",
          "description": "Manage your tabs efficiently.",
          "permissions": ["tabs", "storage"],
          "action": {
            "default_popup": "index.html",
            "default_icon": {
              "16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png"
            }
          },
          "background": {
            "service_worker": "background.js"
          },
          "icons": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
          }
        }